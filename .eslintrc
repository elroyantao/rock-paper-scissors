{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "browser": true,
    "es6": true
  },
  "plugins": [
    "compat"
  ],
  "rules": {
    "semi": [2, "never"],
    "no-unexpected-multiline": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "arrow-body-style": ["error", "as-needed"],
    "react/require-default-props": 0,
    "import/prefer-default-export": 0,
    "arrow-parens": 0
  }
}
